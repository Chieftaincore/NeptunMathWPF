        <Label Content="GEMINI API Key'iniz:" Margin="10,48,267,199"/>
        <RadioButton x:Name="proRadioButton" Content="Pro Versiyon(Sahipseniz)" HorizontalAlignment="Left" Margin="27,145,0,0" VerticalAlignment="Top"/>
